      $(this).parents(".product-details").find(".prod-price").html(var_price);
      if($(this).attr('data-compare-price')){
        var var_copm_price = '  <strike><span style="font-size: 11px; font-weight: 300; display: inline !important;">'+$(this).attr('data-compare-price')+'</span></strike>';
        $(this).parents(".product-details").find(".prod-price").append(var_copm_price);
      }
      var afterPayAmount = $(this).parents(".product-details").find("afterpay-placement");
      afterPayAmount.attr('data-amount',$(this).attr('data-price-val'));
    });
  </script>
  <script>
    $("#shopify-section-collection-template-new .collection-items .product-div .variant-color-swatch").on('click', function(){
      var var_price = $(this).attr('data-price');
      var var_copm_price = $(this).parents(".price-swatch-div").find(".prod-price").html('');
      $(this).parents(".price-swatch-div").find(".product__product-price").html(var_price);
      if($(this).attr('data-compare-price')){
        var var_copm_price = '  <strike><span style="font-size: 11px; font-weight: 300; display: inline !important;">'+$(this).attr('data-compare-price')+'</span></strike>';
        $(this).parents(".price-swatch-div").find(".prod-price").html(var_copm_price);
      }
      
      $("#shopify-section-collection-template-new .discount__badge.right span").show();
      var discount_percent = $(this).attr('data-discount');
      
      if(discount_percent != "" && discount_percent != undefined){
        $(this).parents('.product-div').find(".discount__badge.right").html('<span>'+discount_percent+'</span>');
      } else {
        $(this).parents('.product-div').find(".discount__badge.right span").hide();
      }

    });
  </script>
  <script>
    $("#shopify-section-collection-template-new .collection-items .product-div .variant-color-swatch").on('click', function(){
      var current_variant_qty = $(this).attr('data-inventory-qty');
      var atc_btn = $(this).parents('.product-div').find(".add-to-cart-btn");
      if(current_variant_qty <= 0 ){

        atc_btn.val("Sold Out");
        atc_btn.css("cursor", "not-allowed");
        atc_btn.attr("disabled", true);
      }else {
        atc_btn.val("Add to Cart");
        atc_btn.css("cursor", "pointer");
        atc_btn.attr("disabled", false);

      }
    });
  </script>
  
          
