  <script>
    $("#shopify-section-product-template-new .variant-color-swatch").click(function(){
      var current_variant_qty = $(this).attr('data-inventory-qty');
      if(current_variant_qty <= 0 ){
        console.log('current_variant_qty',current_variant_qty);
        $("#AddToCart").text("Sold Out");
        $("#AddToCart").css("cursor", "not-allowed");
        $("#AddToCart").attr("disabled", true);
        $(".buy-now-btn").hide();
      }
      else {
        $("#AddToCart").text("Add to Cart");
        $("#AddToCart").css("cursor", "pointer");
        $("#AddToCart").attr("disabled", false);
        $(".buy-now-btn").show();
      }
      
      $(".discount__badge.left span").show();
      var discount_percent = $(this).attr('data-discount');
      console.log(discount_percent);
      if(discount_percent){
        $(".discount__badge.left").html('<span>'+discount_percent+'</span>');
      } else {
        $(".discount__badge.left span").hide();
      }

    });
  </script>
