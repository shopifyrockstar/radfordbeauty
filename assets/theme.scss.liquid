//  ======= Font Files ==========
@font-face {
  font-family: "NimbusSans-Regular";
  src: url('{{ "NimbusSansDOT-ReguExte.eot" | asset_url }}');
  src: url('{{ "NimbusSansDOT-ReguExte.eot?#iefix" | asset_url }}')
      format("embedded-opentype"),
    url('{{ "NimbusSansDOT-ReguExte.woff2" | asset_url }}') format("woff2"),
    url('{{ "NimbusSansDOT-ReguExte.woff" | asset_url }}') format("woff"),
    url('{{ "NimbusSansDOT-ReguExte.ttf" | asset_url }}') format("truetype"),
    url('{{ "NimbusSansDOT-ReguExte.svg#NimbusSansDOT-ReguExte" | asset_url }}')
      format("svg");
  font-weight: 100;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: "NimbusSans-Bold";
  src: url('{{ "NimbusSansDOT-BoldExte.eot" | asset_url }}');
  src: url('{{ "NimbusSansDOT-BoldExte.eot?#iefix" | asset_url }}')
      format("embedded-opentype"),
    url('{{ "NimbusSansDOT-BoldExte.woff2" | asset_url }}') format("woff2"),
    url('{{ "NimbusSansDOT-BoldExte.woff" | asset_url }}') format("woff"),
    url('{{ "NimbusSansDOT-BoldExte.ttf" | asset_url }}') format("truetype"),
    url('{{ "NimbusSansDOT-BoldExte.svg#NimbusSansDOT-BoldExte" | asset_url }}')
      format("svg");
  font-weight: 100;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: "LeJeuneText-Regular";
  src: url('{{ "LeJeuneText-Regular.eot" | asset_url }}');
  src: url('{{ "LeJeuneText-Regular.eot?#iefix" | asset_url }}')
      format("embedded-opentype"),
    url('{{ "LeJeuneText-Regular.woff2" | asset_url }}') format("woff2"),
    url('{{ "LeJeuneText-Regular.woff" | asset_url }}') format("woff"),
    url('{{ "LeJeuneText-Regular.ttf" | asset_url }}') format("truetype"),
    url('{{ "LeJeuneText-Regular.svg#LeJeuneText-Regular" | asset_url }}')
      format("svg");
  font-weight: 100;
  font-style: normal;
  font-display: swap;
}
html{
  scroll-behavior: smooth;
}


body {
  margin: 0;
  padding: 0;
  font-family: "NimbusSans-Regular", sans-serif;
  -moz-osx-font-smoothing: grayscale;
          -webkit-font-smoothing: antialiased;
          font-smoothing: antialiased;
          -webkit-font-smoothing: antialiased;
}
a {
  text-decoration: none;
}

input:-webkit-autofill, select:-webkit-autofill {
  -webkit-box-shadow: 0 0 0 30px #fff inset;
  -webkit-text-fill-color: #000 !important;
}




//CUSTOM SCROLL BAR FOR REVIEWS SECTIONS
/* width */
.jdgm-carousel-item__review-body::-webkit-scrollbar, .jdgm-rev__body::-webkit-scrollbar{
  width: 4px;
}

/* Track */
.jdgm-carousel-item__review-body::-webkit-scrollbar-track, .jdgm-rev__body::-webkit-scrollbar-track {
  background: #EEEEEE;
  border-radius: 24px;
}
 
/* Handle */
.jdgm-carousel-item__review-body::-webkit-scrollbar-thumb, .jdgm-rev__body::-webkit-scrollbar-thumb {
  background: #AAAAAA; 
   border-radius: 24px;
}

/* Handle on hover */
.jdgm-carousel-item__review-body::-webkit-scrollbar-thumb:hover, .jdgm-rev__body::-webkit-scrollbar-thumb:hover {
  background: #AAAAAA; 
}


