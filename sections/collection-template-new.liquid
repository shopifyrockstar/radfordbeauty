<div class="boost-pfs-filter-tree-overlay filter-drawer__close active-tab-filter"></div>


<div class="collection-page">
  <div class="container">
    <div class="collection__secondary-header">
      <h1>{{ collection.title }}</h1>
    </div>
    <div class="secondary-header-links">
      <div class="collections-links">
        <ul> 
          <li class="collection-tab tab-active" data-value=".main-collection">
          {%if collection.title == "All" or collection.title == "All Radford Products"%}
            Shop All
            {%else%}
            Shop All {{collection.title}}</li>
          {%endif%}

          
          <!-- 
          {% if collection.handle == 'skincare' %}
          <div class="boost-pfs-filter-tree boost-pfs-filter-tree-v skincare-filter common-header-filter"></div>
          {% elsif collection.handle == 'makeup-1' %}
          <div class="boost-pfs-filter-tree boost-pfs-filter-tree-v common-header-filter"></div>
          {% endif %}
          
          <li class="collection-tab" data-value=".face-collection" linkURL="/collections/face">Face</li>
          <li class="collection-tab" data-value=".eye-collection" linkURL="/collections/eyes">Eye</li>
          <li class="collection-tab" data-value=".lip-collection" linkURL="/collections/lips">Lip</li>
          <li class="collection-tab" data-value=".bundles-collection" linkURL="/collections/bundles">Bundles</li> 
          -->
          
        </ul>
      </div>
    </div>

    <div class="collection-items__wrapper">

      <!-- MAIN collection -->

      <div id="collection-item-1" class="collection-content main-collection active" style="display: block;">

        <div class="collection__sort_filter">

          <div class="filter-sec">
            <div class="collection-filter-button">

              <svg width="16" height="14" viewBox="0 0 16 14" fill="none" xmlns="http://www.w3.org/2000/svg">
